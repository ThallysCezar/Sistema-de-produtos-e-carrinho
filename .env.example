RABBITMQ_PASSWORD=guest

# Portas expostas (ajuste se necessário)
POSTGRES_PORT=5433
RABBITMQ_PORT_EXTERNAL=5673
RABBITMQ_MANAGEMENT_PORT=15673
BACKEND_PORT=8081
FRONTEND_PORT=80
